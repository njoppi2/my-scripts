    //////////////////////////////// R  E  P  L  A  C  E     R  U  L  E  S ///////////////////////////////////////////////
    "replacerules.rules": {
        "Indent svgs": {
            "find": ">",
            "replace": ">\n",
        },
        "Remove trailing and leading whitespace": {
            "find": "^\\s*(.*)\\s*$",
            "replace": "$1"
        },
        "Remove blank lines": {
            "find": "^\\n",
            "replace": "",
            /* "languages": [
						"typescript"
				] */
        },
        "import in ./styles": {
            "find": " } from './styles';",
        },
    },
    "replacerules.rulesets": {
        "Remove lots of stuff": {
            "rules": [
                "Remove blank lines",
                "Remove trailing and leading whitespace",
            ]
        },
        "import in ./styles": {
            "find": " } from './styles';",
            "replace": ", $CLIPBOARD } from './styles';"
        },
    },
